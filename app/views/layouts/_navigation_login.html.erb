<% if user_signed_in? %>
  <li><%= link_to 'Edit account', edit_user_registration_path, class: 'glyphicon glyphicon-edit' %></li>
  <li><%= link_to 'Sign out', destroy_user_session_path, :method=>'delete', class: 'glyphicon glyphicon-log-out' %></li>
<% else %>
  <li><%= link_to ' Login', new_user_session_path, class: 'glyphicon glyphicon-log-in' %></li>
  <li><%= link_to ' Sign up', new_user_registration_path, class: 'glyphicon glyphicon-user' %></li>
<% end %>

  
  
  